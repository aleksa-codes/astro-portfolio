    "@tailwindcss/cli": "^4.0.0-beta.9",
    "@tailwindcss/postcss": "^4.0.0-beta.9",
    "@tailwindcss/vite": "^4.0.0-beta.9",
    "@unpic/astro": "^1.0.0",
    "astro": "5.1.7",
    "astro-capo": "^0.0.1",
    "astro-compress": "^2.3.5",
    "astro-icon": "^1.1.5",
    "astro-og-canvas": "^0.5.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.0.0-beta.9",
    "typescript": "^5.7.2",
    "typewriter-effect": "^2.21.0"
