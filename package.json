  "name": "astro-tailwind-decapcms-starterkit",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "start": "astro dev",
    "sync": "astro sync",
    "build:tailwind": "npx @tailwindcss/cli -i src/styles/global.css -o dist/global.css --minify",
    "build:postcss": "postcss dist/global.css -o dist/global.css",
    "build": "astro check && astro build && yarn build:tailwind && yarn build:postcss",
    "preview": "astro preview",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md,css,html,yaml,astro}\"",
    "decap": "npx decap-server"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.0.6",
    "@astrojs/rss": "^4.0.11",
